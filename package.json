{
    "name": "nodejscrud",
    "version": "1.0.0",
    "description": "Um projeto de CRUD com Nodejs e React",
    "main": "server.js",
    "scripts": {
        "tsnode": "ts-node-dev src/server",
        "front": "cd ./src/front && npm start",
        "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
