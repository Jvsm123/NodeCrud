        "mysql2": "^2.3.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.41"
    },
    "devDependencies": {
        "@types/ejs": "^3.1.0",
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.10",
        "ts-node": "^10.4.0",
        "ts-node-dev": "^1.1.8"
    }
